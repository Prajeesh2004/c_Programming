#include <stdio.h>
void main() {
    int num1,num2;
    char op;

    printf("Enter num1: ");
    scanf("%d",&num1);
    
    printf("Enter num2: ");
    scanf("%d",&num2);

    printf("Enter op: ");
    scanf("\n %c",&op);

    if(op=='+')
    {
        printf("sum: %d",num1+num2);
    }
    else if(op=='-')
    {
        printf("diff: %d",num1-num2);
    }
    else if(op=='*')
    {
        printf("product: %d",num1*num2);
    }
    else if(op=='/')
    {
        printf("quotient: %d",num1/num2);
    }
    else
    {
        printf("Invalid Operator");
    }
}
